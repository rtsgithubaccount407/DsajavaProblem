import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
	public static void main (String[] args) throws java.lang.Exception
	{
                Scanner sc = new Scanner(System.in);

                // User input
                int N = sc.nextInt();              

                // Step 1
                if(N<30)          
                {
                        System.out.println("less important");
                }
                else                                              
                {
                        System.out.println("more important");
                }
                
	}
}
